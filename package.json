  "name": "storewithstripe",
  "version": "1.0.0",
  "description": "A simple store with stripe",
  "main": "index.js",
  "scripts": {
    "install:front": "cd ./frontend && npm install",
    "install:back": "cd ./backend && npm install",
    "install:apps": "npm run install:front && npm run install:back",
    "compose:up": "cd backend && docker-compose up -d --build",
